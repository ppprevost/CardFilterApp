{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "no-console": true,
    "prefer-for-of": false,
    "quotemark": false,
    "ordered-imports": false,
    "class-name": false,
    "object-literal-sort-keys": false,
    "interface-name": [true, "never-prefix"]
  },
  "rulesDirectory": []
}
